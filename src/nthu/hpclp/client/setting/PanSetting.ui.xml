	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:m="urn:import:gwt.material.design.client.ui">
<ui:style>
</ui:style>

<m:MaterialPanel ui:field="root">
	<!-- Header Section -->
	<m:MaterialHeader depth="996" layoutPosition="FIXED" width="100%">
		<!-- NavBar Component -->
		<m:MaterialNavBar ui:field="navAppBar" />
	</m:MaterialHeader>
	
	<!-- body section -->
	<m:MaterialContainer paddingTop="70">
		<m:MaterialRow backgroundColor="GREEN">
			<m:MaterialColumn grid="s12">
				<m:MaterialTab backgroundColor="GREEN" indicatorColor="GREEN_LIGHTEN_3" waves="YELLOW" ui:field="tab">
					<m:MaterialTabItem waves="YELLOW" grid="s2"> <m:MaterialLink text="輻射單位" href="#tab1" textColor="WHITE" ui:field="lnkTab1" /> </m:MaterialTabItem>
					<m:MaterialTabItem waves="YELLOW" grid="s2"> <m:MaterialLink text="儀器類型" href="#tab2" textColor="WHITE" ui:field="lnkTab2" /> </m:MaterialTabItem>
					<m:MaterialTabItem waves="YELLOW" grid="s2"> <m:MaterialLink text="服務項目" href="#tab3" textColor="WHITE" ui:field="lnkTab3" /> </m:MaterialTabItem>
					<m:MaterialTabItem waves="YELLOW" grid="s2"> <m:MaterialLink text="射源種類" href="#tab4" textColor="WHITE" ui:field="lnkTab4" /> </m:MaterialTabItem>
					<m:MaterialTabItem waves="YELLOW" grid="s2"> <m:MaterialLink text="還原點" href="#tab5" textColor="WHITE" ui:field="lnkTab5" /> </m:MaterialTabItem>
				</m:MaterialTab>
			</m:MaterialColumn>
			<m:MaterialColumn m:id="tab1" grid="s12" paddingTop="13" ui:field="archTab1"/>
			<m:MaterialColumn m:id="tab2" grid="s12" paddingTop="13" ui:field="archTab2"/>
			<m:MaterialColumn m:id="tab3" grid="s12" paddingTop="13" ui:field="archTab3"/>
			<m:MaterialColumn m:id="tab4" grid="s12" paddingTop="13" ui:field="archTab4"/>
			<m:MaterialColumn m:id="tab5" grid="s12" paddingTop="13" ui:field="archTab5"/>
		</m:MaterialRow>
	</m:MaterialContainer>

</m:MaterialPanel>

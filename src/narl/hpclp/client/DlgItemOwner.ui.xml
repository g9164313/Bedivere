<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:g2="urn:import:com.googlecode.gwt.charts.client.calendar"
	xmlns:m="urn:import:gwt.material.design.client.ui" >
	<ui:style>
	
	</ui:style>
	
<m:MaterialModal width="65%" type="DEFAULT" dismissable="true" inDuration="500" outDuration="500" ui:field="root" >
	<m:MaterialModalContent >
	
		<m:MaterialRow>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxKey" placeholder="公司代號" width="5em"/>
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxName" placeholder="公司名稱" width="20em"/>
			</m:MaterialColumn>

		</m:MaterialRow>
		
		<m:MaterialRow>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxZip" placeholder="郵遞區號" width="5em"/>
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxAddr" placeholder="公司地址" width="20em"/>				
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxPhon" placeholder="聯絡電話" width="11em"/>
			</m:MaterialColumn>
		</m:MaterialRow>
		
		<m:MaterialRow>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxDept" placeholder="部門" width="5em"/>
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxPern" placeholder="聯絡(委託)人" width="8em"/>
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxMail" placeholder="E-mail" width="13em"/>
			</m:MaterialColumn>
			<m:MaterialColumn>
				<m:MaterialTextBox ui:field="boxMemo" placeholder="備註" width="8em"/>
			</m:MaterialColumn>
		</m:MaterialRow>
		
	</m:MaterialModalContent>
	<m:MaterialModalFooter>		
		<m:MaterialButton text="確認" type="FLAT" ui:field="btnAction" />
		<m:MaterialButton text="取消" type="FLAT" ui:field="btnCancel" />
	</m:MaterialModalFooter>
</m:MaterialModal>

</ui:UiBinder> 
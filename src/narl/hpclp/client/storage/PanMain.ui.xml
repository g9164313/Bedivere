<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder 
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:g2="urn:import:com.googlecode.gwt.charts.client.calendar"
	xmlns:m="urn:import:gwt.material.design.client.ui" >
	
<ui:style>
	@external side-nav, active;
    body{
        background-color: #e9e9e9;
        overflow: auto !important;
    }
    .side-nav li.active{
        border-left: 4px solid #757575;
    }
    .bordered{
        border-left: 2px solid #e9e9e9;
    }
    .animation{
        transition: 0.4s all;
        -webkit-transition: 0.4s all;
        -moz-transition: 0.4s all;
    }
</ui:style>

<m:MaterialPanel ui:field="root">

        <!-- Header Section -->
        <m:MaterialHeader depth="996" layoutPosition="FIXED" width="100%">
            <!-- NavBar Component -->
            <m:MaterialNavBar ui:field="navApp" >            	
                <m:MaterialNavSection float="LEFT" hideOn="NONE">
                	<m:MaterialLink iconPosition="NONE" tooltip="排程" tooltipPosition="BOTTOM" iconType="EVENT" ui:field="lnkPanMeet" />
                	<m:MaterialLink iconPosition="NONE" tooltip="報告" tooltipPosition="BOTTOM" iconType="DESCRIPTION" ui:field="lnkPanProdx" />
                	<m:MaterialLink iconPosition="NONE" tooltip="會計" tooltipPosition="BOTTOM" iconType="ACCOUNT_BALANCE" ui:field="lnkPanAccnt" />
                	<!-- <m:MaterialLink iconPosition="NONE" tooltip="資料" tooltipPosition="BOTTOM" iconType="CLOUD_QUEUE" ui:field="lnkPanStorage" /> -->
                </m:MaterialNavSection>
                <m:MaterialNavSection float="RIGHT" hideOn="NONE">   
                </m:MaterialNavSection>
            </m:MaterialNavBar>
        </m:MaterialHeader>
 
        <!-- Container -->
        <m:MaterialContainer paddingTop="70" paddingLeft="7" paddingRight="7">
        <m:MaterialRow>
        	<m:MaterialColumn backgroundColor="white" shadow="1">
        		<g:HTMLPanel ui:field="arch1" />
        		<g:HTMLPanel ui:field="arch2" />
        	</m:MaterialColumn>
        	<m:MaterialColumn backgroundColor="white" shadow="1">
        		<g:HTMLPanel ui:field="arch3" />
        		<g:HTMLPanel ui:field="arch4" />
        	</m:MaterialColumn>
        	<m:MaterialColumn backgroundColor="white" shadow="1">
        		<g:HTMLPanel ui:field="arch5" />
        		<g:HTMLPanel ui:field="arch6" />
        	</m:MaterialColumn>
        </m:MaterialRow>
	    </m:MaterialContainer>
</m:MaterialPanel>

</ui:UiBinder> 